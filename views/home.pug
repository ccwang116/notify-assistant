extends layout.pug

block content
    .list-group.w-auto
        each row in results
          a.list-group-item.list-group-item-action.d-flex.gap-3.py-3(href=row.url)
            .d-flex.gap-2.w-100.justify-content-between
              h3.mb-0=row.title
              small.opacity-50.text-nowrap.btn go settings